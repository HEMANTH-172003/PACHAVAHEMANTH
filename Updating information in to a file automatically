name=str(input('enter ur name:'))
adminno=str(input('enter ur admin no:'))
clg_name=str(input('enter ur clg name:'))
course_name=str(input('enter ur course name:'))
phno=str(input('enter ur ph no :'))
email=str(input('enter ur mail id'))
fd = open('sample.txt','w')
fd.write(name)
fd.write('\n')
fd.write(adminno)
fd.write('\n')
fd.write(clg_name)
fd.write('\n')
fd.write(course_name)
fd.write('\n')
fd.write(phno)
fd.write('\n')
fd.write(email)
fd.close()
